#include <iostream>

int main() {
  std::cout << "Enter no. of rows and columns: \n";
  int r,c;
  std::cin>>r>>c;
  int arr[r][c];
  for(int i = 0; i < r; i++) {
    for(int j = 0; j < c; j++) {
      arr[i][j] = i * j;
    }
  }
  for(int i=0;i<r;i++){
    for(int j=0;j<c;j++){
      std::cout<<arr[i][j]<<" ";
    }
    std::cout << "\n";
  }
}
