#include <iostream>

int main() {
  std::string x;
  std::cout << "Enter the month name: ";
  std::cin>> x;
  if(x == "January" || x == "january" || x == "March" || x == "march" || x == "May" || x == "may" || x == "July" || x == "july" || x == "August" || x == "august"|| x== "October" || x== "october" || x== "December" || x== "december"){
    std::cout<< "31 days";
  }
  else if(x == "February" || x == "february"){
    std::cout<< "28/29 days";
  }
  else if(x == "April" || x == "april" || x == "June" || x == "june" || x == "September" || x == "september" || x == "November" || x == "november"){
    std::cout<< "30 days";
  }
  else{
    std::cout<<"Invalid input";
  }
  
  
}
